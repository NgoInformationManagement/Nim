{% extends 'NIMWebBundle::layout.html.twig' %}

{% import 'NIMFormBundle:Bootstrap3:button.html.twig' as buttons %}
{% from 'NIMFormBundle:Bootstrap3:pagination/resource_bundle.html.twig' import pagination %}
{% from 'NIMWebBundle:Agency:macro.html.twig' import list %}

{% block page_name %}
    <i class="icon-rocket"></i>
    {{ 'agency.title.index'|trans }}
{% endblock page_name %}


{% block page_action %}
    {{ buttons.new(path('nim_agency_create'), 'agency.button.new') }}
{% endblock page_action %}


{% block content %}
    {{ pagination(agencies) }}
    {{ list(agencies) }}
    {{ pagination(agencies) }}
{% endblock %}