{
    "name": "nim/theme-bundle",
    "type": "symfony-bundle",
    "description": "Nim theme bundle : Manage your symfony theme simpler",
    "keywords": ["nim", "ngo", "humanitarian", "theme", "select2", "datepicker"],
    "homepage": "https://github.com/NgoInformationManagement/FormBundle",
    "license": "MIT",

    "authors": [
        {
            "name": "aRn0D",
            "homepage": "https://github.com/aRn0D"
        }
    ],

    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },

    "target-dir": "Nim/ThemeBundle",

    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.3",
        "knplabs/knp-menu-bundle": "~2.0"
    },

    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },

    "config": {
        "bin-dir": "bin"
    },

    "autoload": {
        "psr-0": {
            "Nim\\ThemeBundle": ""
        }
    }
}