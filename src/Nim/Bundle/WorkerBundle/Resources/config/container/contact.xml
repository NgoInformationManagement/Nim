<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
http://symfony.com/schema/dic/services/services-1.0.xsd">



    <services>
        <!-- Contact Form -->
        <service id="nim.form.type.contact" class="%nim.form.type.contact.class%">
            <call method="setValidationGroups">
                <argument>%nim.validation_group.contact%</argument>
            </call>
            <call method="setDataClass">
                <argument>%nim.model.contact.class%</argument>
            </call>
            <tag name="form.type" alias="nim_contact" />
        </service>
    </services>
</container>
